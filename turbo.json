{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["build/**", ".next/**"],
      "env": [
        "VITE_CLERK_PUBLISHABLE_KEY",
        "VITE_HOST_API",
        "VITE_HOST_API_PROTOCOL",
        "UPSTASH_QSTASH_TOKEN",
        "UPSTASH_REDIS_TOKEN",
        "UPSTASH_REDIS_URL",
        "FUNCTION_BASE_URL",
        "SPOTIFY_CLIENT_ID",
        "SPOTIFY_CLIENT_SECRET",
        "CLERK_SECRET_KEY",
        "CLERK_PUBLISHABLE_KEY",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY"
      ]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": true,
      "env": [
        "VITE_CLERK_PUBLISHABLE_KEY",
        "VITE_HOST_API",
        "VITE_HOST_API_PROTOCOL",
        "UPSTASH_QSTASH_TOKEN",
        "UPSTASH_REDIS_TOKEN",
        "UPSTASH_REDIS_URL",
        "FUNCTION_BASE_URL",
        "SPOTIFY_CLIENT_ID",
        "SPOTIFY_CLIENT_SECRET",
        "CLERK_SECRET_KEY",
        "CLERK_PUBLISHABLE_KEY",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "VITE_CLERK_PUBLISHABLE_KEY",
        "VITE_HOST_API",
        "VITE_HOST_API_PROTOCOL",
        "UPSTASH_QSTASH_TOKEN",
        "UPSTASH_REDIS_TOKEN",
        "UPSTASH_REDIS_URL",
        "FUNCTION_BASE_URL",
        "SPOTIFY_CLIENT_ID",
        "SPOTIFY_CLIENT_SECRET",
        "CLERK_SECRET_KEY",
        "CLERK_PUBLISHABLE_KEY",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY"
      ]
    }
  }
}
